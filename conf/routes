# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /randam						controllers.Application.randam
GET		/task						controllers.Application.task
#POST	/task						controllers.Application.taskSubmit
POST	/taskDelete					controllers.Application.taskDelete
GET		/socket						controllers.WebSocketController.socket
GET		/wsTask					controllers.TaskController.wsTask

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
